<link rel="stylesheet" href="/css/transactions.css" />
<table>
    <tr>
        <th>Customer Name</th>
        {{!-- <th>Customer ID</th> --}}
        <th>Customer Email</th>
        <th>Confirmation No</th>
        <th>Payment Date</th>
        <th>Course</th>
        <th>Amount</th>
    </tr>
</table>
{{#each allList}}
<table>
    <tr>
        <td>{{this.name}}</td>
        {{!-- <td>{{this.id}}</td> --}}
        <td>{{this.email}}</td>
        <td>{{this.no}}</td>
        <td>{{this.date}}</td>
        <td>{{this.class}}</td>
        <td>{{this.total}}</td>
    </tr>
</table>
{{/each}}